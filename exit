Indexes:
    "agents_pkey" PRIMARY KEY, btree (id)
    "agents_user_id_key" UNIQUE CONSTRAINT, btree (user_id)
    "ix_agents_agent_id_3cx" UNIQUE, btree (agent_id_3cx)
    "ix_agents_extension" UNIQUE, btree (extension)
    "ix_agents_is_logged_in" btree (is_logged_in)
    "ix_agents_status" btree (status)
Foreign-key constraints:
    "agents_supervisor_id_fkey" FOREIGN KEY (supervisor_id) REFERENCES users(id)
    "agents_user_id_fkey" FOREIGN KEY (user_id) REFERENCES users(id)
    "fk_agents_current_call" FOREIGN KEY (current_call_id) REFERENCES calls(id)
Referenced by:
    TABLE "calls" CONSTRAINT "calls_agent_id_fkey" FOREIGN KEY (agent_id) REFERENCES agents(id)
    TABLE "leads" CONSTRAINT "leads_assigned_agent_id_fkey" FOREIGN KEY (assigned_agent_id) REFERENCES agents(id)

